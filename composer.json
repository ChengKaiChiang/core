{
    "name": "oidc/core",
    "description": "OpenID Connect Core Library implementation for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oidc",
        "openid connect"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/support": "^5.6",
        "league/oauth2-client": "^2.4",
        "web-token/jwt-framework": "^1.3"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^6 | ^7 | ^8",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\": "app"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}

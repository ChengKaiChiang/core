{
    "name": "oidc/core",
    "description": "OpenID Connect Core Library implementation for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oidc",
        "openid connect"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "web-token/jwt-checker": "^1.3",
        "web-token/jwt-core": "^1.3",
        "web-token/jwt-signature": "^1.3"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^7 | ^8",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.3",
        "symfony/yaml": "^4.3",
        "web-token/jwt-console": "^1.3",
        "web-token/jwt-encryption": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\": "src/OpenIDConnect"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\": "app"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}

{
    "name": "oidc/core",
    "description": "OpenID Connect Core Library implementation for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oidc",
        "openid connect"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/oidcphp/core/issues"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "mileschou/psr": "^1.3",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "web-token/jwt-checker": "^1.3",
        "web-token/jwt-core": "^1.3",
        "web-token/jwt-encryption": "^1.3",
        "web-token/jwt-signature": "^1.3"
    },
    "suggest": {
        "laminas/laminas-diactoros": "Require if use Laravel and Laminas diactoros"
    },
    "require-dev": {
        "illuminate/container": "^5.6 | ^6 | ^7",
        "illuminate/support": "^5.6 | ^6 | ^7",
        "laminas/laminas-diactoros": "^2.2",
        "mileschou/mocker": "^1.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^7 | ^8",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^4.3",
        "symfony/yaml": "^4.3",
        "web-token/jwt-console": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}

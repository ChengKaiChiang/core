{
    "name": "oidc/client",
    "description": "OpenID Connect Relying Party (RP, Client) implementation for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oidc",
        "openid connect"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6 | ^7 | ^8",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.8"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
